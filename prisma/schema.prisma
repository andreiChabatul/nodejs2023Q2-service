// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id String @id @default(uuid())
  login String
  version Int @default(1)
  createdAt DateTime   @default(now()) 
  updatedAt DateTime   @default(now()) 
  password String
}

model Artist {
  id String @id @default(uuid())
  name String
  grammy Boolean
  favorites Boolean @default(false)
}

model Album {
  id String @id @default(uuid())
  name String
  year Int
  artistId String?
  favorites Boolean @default(false)
}

model Track {
  id String @id @default(uuid())
  name String
  artistId String?
  albumId String?
  duration Int
  favorites Boolean @default(false)
}

model UserData {
  id String @id @default(uuid())
  login String @unique
  password String
}

model TokenBase {
    refreshToken: String;
    id: String;
}